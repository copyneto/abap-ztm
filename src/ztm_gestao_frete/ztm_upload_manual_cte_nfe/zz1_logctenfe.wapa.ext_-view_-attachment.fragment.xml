<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
	xmlns:u="sap.ui.unified"                                                                                                                                                                                                                                      
xmlns:upload="sap.m.upload">                                                                                                                                                                                                                                   
<Dialog id="attachmentDialog" icon="sap-icon://attachment" title="{i18n>DialogDownloadTitle}">&nbsp;                                                                                                                                                           
	<upload:UploadSet                                                                                                                                                                                                                                             
				id="UploadSet"                                                                                                                                                                                                                                             
				instantUpload="false"                                                                                                                                                                                                                                      
				showIcons="true"                                                                                                                                                                                                                                           
				uploadEnabled="true"                                                                                                                                                                                                                                       
				terminationEnabled="true"                                                                                                                                                                                                                                  
				fileTypes="xml"                                                                                                                                                                                                                                            
				afterItemAdded=".onAfterItemAdded"                                                                                                                                                                                                                         
				uploadCompleted=".onUploadCompleted"                                                                                                                                                                                                                       
				fileTypeMismatch=".onFileTypeMismatch"                                                                                                                                                                                                                     
				selectionChanged="onSelectionChangeAttachment"                                                                                                                                                                                                             
				uploadUrl="/sap/opu/odata/sap/ZTM_RECEB_MANUAL_CTE_NFE_SRV/documentSet"                                                                                                                                                                                    
				items="{to_MovAttach}">                                                                                                                                                                                                                                    
				<upload:toolbar>                                                                                                                                                                                                                                           
			        <OverflowToolbar>                                                                                                                                                                                                                                   
            			<!-- <CheckBox id="checkbox" select=".onSelectAllAttachments" visible="true" text="{i18n>ButtonSelectAll}"  />  -->&nbsp;                                                                                                                       
                        <HBox class="radioDocTypeBox">                                                                                                                                                                                                         
                            <RadioButtonGroup id="radioDocType" columns="2" valueState="Information" visible="true">                                                                                                                                           
                                <RadioButton id="nfe" text="NF-e" selected="true" />                                                                                                                                                                           
                                <RadioButton id="cte" text="CT-e" />                                                                                                                                                                                           
                            </RadioButtonGroup>&nbsp;                                                                                                                                                                                                          
			            </HBox>                                                                                                                                                                                                                                         
                        <ToolbarSpacer/>                                                                                                                                                                                                                       
            			<Button id="remove" enabled="false" icon="sap-icon://delete" press="onRemove" type="Transparent" visible="false" /> <!-- Botão desabilitado -->                                                                                                 
            			<Button id="download" enabled="false" icon="sap-icon://download" press="onDownload" type="Transparent" visible="false" /> <!-- Botão desabilitado -->&nbsp;                                                                                     
        			</OverflowToolbar>                                                                                                                                                                                                                                  
    			</upload:toolbar>                                                                                                                                                                                                                                       
				<upload:UploadSetItem                                                                                                                                                                                                                                      
							fileName="{Filename}"                                                                                                                                                                                                                                   
							mediaType="{mediaType}"                                                                                                                                                                                                                                 
							enabledEdit="false"                                                                                                                                                                                                                                     
							visibleEdit="false"                                                                                                                                                                                                                                     
							enabledRemove="false"                                                                                                                                                                                                                                   
							visibleRemove="false"                                                                                                                                                                                                                                   
							thumbnailUrl="{ path: 'Filename', formatter: '.getIcon' }"                                                                                                                                                                                              
							openPressed=".onOpenPressed"                                                                                                                                                                                                                            
						>                                                                                                                                                                                                                                                        
						<upload:attributes>                                                                                                                                                                                                                                      
							<ObjectAttribute                                                                                                                                                                                                                                        
								title="{i18n>DMSDocument}"                                                                                                                                                                                                                             
								text="{Documentnumber}"                                                                                                                                                                                                                                
								active="false"                                                                                                                                                                                                                                         
							/>                                                                                                                                                                                                                                                      
							<ObjectAttribute                                                                                                                                                                                                                                        
								title="{i18n>DMSType}"                                                                                                                                                                                                                                 
								text="{Documenttype}"                                                                                                                                                                                                                                  
								active="false"                                                                                                                                                                                                                                         
							/>                                                                                                                                                                                                                                                      
							<ObjectAttribute                                                                                                                                                                                                                                        
								title="{i18n>DMSVersion}"                                                                                                                                                                                                                              
								text="{Documentversion}"                                                                                                                                                                                                                               
								active="false"                                                                                                                                                                                                                                         
							/>                                                                                                                                                                                                                                                      
							<ObjectAttribute                                                                                                                                                                                                                                        
								title="{i18n>DMSUploadedBy}"                                                                                                                                                                                                                           
								text="{CreatedBy}"                                                                                                                                                                                                                                     
								active="false"                                                                                                                                                                                                                                         
							/>                                                                                                                                                                                                                                                      
							<ObjectAttribute                                                                                                                                                                                                                                        
								title="{i18n>DMSUploadedOn}"                                                                                                                                                                                                                           
								text="{ path: 'CreatedAt', formatter: '.getDate' }"                                                                                                                                                                                                    
								active="false"                                                                                                                                                                                                                                         
							/>                                                                                                                                                                                                                                                      
						</upload:attributes>                                                                                                                                                                                                                                     
					</upload:UploadSetItem>                                                                                                                                                                                                                                   
	</upload:UploadSet>                                                                                                                                                                                                                                           
	<beginButton>                                                                                                                                                                                                                                                 
		<Button text="{i18n>ButtonDialogOk}" press=".onAccept" type="Emphasized" enabled="false" id="btnOk" />                                                                                                                                                       
	</beginButton>                                                                                                                                                                                                                                                
	<endButton>                                                                                                                                                                                                                                                   
		<Button text="{i18n>ButtonDialogClose}" press=".onClose" type="Reject" />                                                                                                                                                                                    
	</endButton>                                                                                                                                                                                                                                                  
</Dialog>                                                                                                                                                                                                                                                      
</c:FragmentDefinition>                                                                                                                                                                                                                                        