sap.ui.define([],                                                                                                                                                                                                                                              
    function () {                                                                                                                                                                                                                                              
        "use strict";                                                                                                                                                                                                                                          
        return {                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
            // Função DEFAULT chamada ao inicializar aplicativo                                                                                                                                                                                                
            onInitSmartFilterBarExtension: function (oEvent) {                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
                debugger;                                                                                                                                                                                                                                      
                var vTomorrow  = new Date();                                                                                                                                                                                                                   
                vTomorrow.setDate( vTomorrow.getDate() + 1 );                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
                /* ===================================================================&nbsp;                                                                                                                                                                   
                Monta filtros de seleção com valores default                                                                                                                                                                                                   
                =================================================================== */                                                                                                                                                                         
                var oFilters = {                                                                                                                                                                                                                               
                    "session_data": {                                                                                                                                                                                                                          
                        "ranges": [{                                                                                                                                                                                                                           
                            "exclude": false,                                                                                                                                                                                                                  
                            "operation": "EQ",                                                                                                                                                                                                                 
                            "keyField": "session_data",                                                                                                                                                                                                        
                            "value1": vTomorrow                                                                                                                                                                                                                
                        }]                                                                                                                                                                                                                                     
                    },                                                                                                                                                                                                                                         
                };                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                /* ===================================================================&nbsp;                                                                                                                                                                   
                Atualiza filtros de seleção                                                                                                                                                                                                                    
                =================================================================== */                                                                                                                                                                         
                var oGlobalFilter = oEvent.getSource();                                                                                                                                                                                                        
                oGlobalFilter.setFilterData(oFilters);                                                                                                                                                                                                         
            },                                                                                                                                                                                                                                                 
        onaction: function(oEvent) {                                                                                                                                                                                                                           
            alert('onaction');                                                                                                                                                                                                                                 
        }                                                                                                                                                                                                                                                      
    };                                                                                                                                                                                                                                                         
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               